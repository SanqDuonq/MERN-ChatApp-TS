{
    "version": 2,
    "builds": [
        {
            "src": "./index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["routes/**", "controllers/**", "middlewares/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.ts"
        }
    ]
}